<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mainviewmodel="clr-namespace:SistemaMirno.UI.ViewModel.Main"
                    xmlns:main="clr-namespace:SistemaMirno.UI.View.Main">

    <DataTemplate DataType="{x:Type mainviewmodel:LoginViewModel}">
        <main:LoginView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type mainviewmodel:BranchSelectionViewModel}">
        <main:BranchSelectionView />
    </DataTemplate>
</ResourceDictionary>